using System.Threading;
using System;

class Program
{
	static void Main(string[] args){
		Thread t = new Thread(Method1);
		t.Start();
		Thread t2 = new Thread(Method2);
		t2.Start();
	}
	
	static void Method1(){
		for(int i =0;i<10;i++)
		{
			Console.WriteLine("Method1");
		}
	}
	
	static void Method2(){
		for(int j =0;j <10;j++){
			Console.WriteLine("Method2");
		}
	}
}
	
